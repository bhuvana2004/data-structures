#include<stdio.h>
int main()
{
    int a[100],i,n,x,flag;
    printf("enter number of elements in th array|n");
    scanf("%d",&n);
    printf("enter %d numbers",n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("enter element to be searched");
    scanf("%d",&x);
    for(i=0;i<n;i++)
    {
        if(a[i]==x)
        {
         flag=1;
         break;
        }
    }
    if(flag==1)
    {
        printf("element is found");
    }
    else
    {
        printf("element not found");
    }
    
}
